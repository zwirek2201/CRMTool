<UserControl x:Class="Licencjat_new.Windows.ScanningWindows.ScanningManual"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Name="gdManualModeGrid" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="Transparent">
        <Image Height="100" VerticalAlignment="Top" Margin="180,20,180,0" Source="pack://application:,,,/resources/scan_manual.png" RenderOptions.BitmapScalingMode="HighQuality"/>
        <Button x:Name="btnManualScan" Content="SKANUJ" Width="90" Height="40" BorderThickness="0,0,0,1" BorderBrush="#c2c2c2" VerticalAlignment="Top" Margin="0,120,0,0"></Button>
    </Grid>
</UserControl>
